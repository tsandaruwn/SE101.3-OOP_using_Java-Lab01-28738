package com.mycompany.practical1;
public class Practical1 
{
    public static void main(String[] args) 
    {
        int i=0,j=0;
        for(i=0;i<5;i++)
        {
            System.out.println("Executing Loop "+i);
        }
        
        System.out.println("");
        
        while(j<5)
        {
            System.out.println("Executing Loop "+j);
            j++;
        }
    }
}
